#include <stdio.h>

int main() {
    // Write C code here
    char s[10];
    scanf("%s", s);
    int pos;
    scanf("%d", &pos);
    char ch;
    getchar();
    scanf("%c", &ch);
    printf("dlestx");
    for(int i=0;i<(pos-1);i++){
        printf("%c", s[i]);
    }
    printf("dle%cdle", ch);
    for(int i=(pos-1);s[i]!='\0';i++){
        printf("%c", s[i]);
    }
    printf("dleetx");
}
